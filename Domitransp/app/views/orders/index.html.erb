<div class="row">
                <div class="col-lg-12">
                    <div class="ibox ">
                        <div class="ibox-title">
                            <h5> Envios </h5>
                        </div>
                        <div class="ibox-content">

                            <table class="table table-bordered">
                                <thead>
                                <tr>
                                  <th>Fecha</th>
                                  <th>Consecutivo</th>
                                  <th>Descripcion </th>
                                  <th>Origen</th>
                                  <th>Destino</th>
                                </tr>
                                </thead>
                                <tbody>
                                  <% @orders.each do |order| %>
                                    <tr>
                                      <td><%= order.fecha %></td>
                                      <td><%= order.consecutivo %></td>
                                      <td><%= order.description.objeto %></td>
                                      <td><%= order.beginning.ciudad %></td>
                                      <td><%= order.destination.ciudad %></td>

                                    </tr>
                                  <% end %>
                                </tbody>      
                        </div>
                    </div>
                </div>
