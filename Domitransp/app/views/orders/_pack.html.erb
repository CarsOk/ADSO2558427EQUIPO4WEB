<div id="paquete" class="contenedor-paquete">
  <tr id="paquete">
    <td id="contenedor-input-anidado">
      <label class="label-anidado">Tipo: </label>
        <%= f.input :tipo, as: :select, collection: ['Sobre', 'Paquete', 'Cava', 'Caja', 'Maleta', 'Contenedor'], prompt: 'Selecciona una opciÃ³n', input_html: { class: 'form-control', id: 'my-select' }, label: false %>
      </td>
    <td id="contenedor-input-anidado">
      <label class="label-anidado label-cantidad">Cantidad: </label>
      <%= f.input :cantidad, label: false, input_html: { id: 'input-anidado-cantidad' }, wrapper: false %>
    </td>
    <td id="contenedor-boton-borrar">
      <div class= "div-boton-borrar">
        <%= f.input_field :_destroy, as: :hidden %>
        <%= link_to 'Borrar', '#', id: 'boton-borrar' %>
      </div>
    </td>
  </tr>
</div>