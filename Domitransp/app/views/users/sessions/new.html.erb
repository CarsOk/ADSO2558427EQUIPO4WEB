<h3 class="text-center mb-4">Sign Inasdasdasd</h3>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) { class: 'login-form' } do |f| %>
  <div class="form-group">
    <%= f.input :email,
                required: false,
                autofocus: true,
                input_html: { autocomplete: "email" } %>
  </div>
  <h1> holaa </h1>
  <div class="form-group d-flex">
    <%= f.input :password,
                required: false,
                input_html: { autocomplete: "current-password" } %>
    asdas
  </div>
  <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? input_html: { class: 'checkbox-wrap checkbox-primary' } %>
  <div class="form-actions">
    <%= f.button :submit, "Log in", input_html: { class: 'form-control btn btn-primary rounded submit px-3' } %>
  </div>
<% end %>

<%= render "users/shared/links" %>
